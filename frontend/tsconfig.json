{
   "compileOnSave": true,
   "compilerOptions": {
      "module": "CommonJS",
      "esModuleInterop": true,
      "target": "ES6",
      "types": ["vite/client"],
      "strict": false,
      "noImplicitAny": true,
      "moduleResolution": "Node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
         "*": [ 
            "node_modules/*"
         ]
      }
   },
   "include": [
      "src/**/*"
   ]
}